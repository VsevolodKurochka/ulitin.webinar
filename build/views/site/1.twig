{% import 'macros/macros.base.twig' as macros %}
{% import "macros/macros.landing.twig" as landing %}
{% set brand = 1 %}
{% set title = "Почему я живу по<br>сценарию и мои<br>отношения<br>с мужчинами<br>краткосрочны??" %}

{% extends 'layouts/base.twig' %}

{% block meta %}
	{% set description = 'Зарегистрируйтесь на бесплатный онлайн вебинар' %}
	{% set image = 'img/logo-#{brand}.png' %}
	{% set url = 'url' %}
	{{ macros.meta(title|striptags, description, image, url) }}
{% endblock %}

{% block css %}
	{{ macros.css('css/style.min.css') }}
{% endblock %}

{% block content %}
	
	{{
		landing.landing(
			{
				"brand": brand,
				"image": "img/bg-#{brand}.jpg",
				"logo": "img/logo-#{brand}.png",
				"date": "17 сентября 2018 / 20:00",
				"title": title,
				"form": form,
				"campaign_token": "n0wNk",
				"thankyou_url": "http://promo.markbarton.ru/mk/1/thx",
			}
		)
	}}
{% endblock %}